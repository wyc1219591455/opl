<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="me.zhengjie.modules.opl.mapper.ProblemMapper">

    <insert id="createProblem" parameterType="me.zhengjie.modules.opl.domain.Problem">
        insert into opl_problem
        (number,matter_date,project_id,description,fae_person,model_id,dept_id)
        values
        (#{number},#{matterDate},#{projectId},#{description},#{faePerson},#{modelId},#{deptId})
    </insert>


</mapper>